<div class="container">
  <div class="row">
    <div class="col">
      <img src='/images/logo.png'alt='s' height="80px">
    </div>
    <div class="col-6">
    </div>
    <div class="col">
      <ul class='nav justify-content-end'>
        {{#if user}}
        <li style='color: white' class='nav-link user-nav'>{{user.name}}</li>
        <li class='nav-item'><a
          style='color: white'
          class='nav-link log-nav active '
          aria-current='page'
          href='/user/logout'
        >logout</a></li>
        {{else}}
        <li class='nav-item'><a
          style='color: white'
          class='nav-link log-nav active '
          aria-current='page'
          href='/user/login'
        >login</a></li>
        <li class='nav-item'><a
          style='color: white'
          class='nav-link log-nav active'
          aria-current='page'
          href='/home'
        >home</a></li>
        {{/if}}
      </ul>
    </div>
  </div>
